# ESPHome Secrets Template
# Copy this to secrets.yaml and fill in your actual values

# WiFi Configuration
wifi_ssid: "your_wifi_network_name"
wifi_password: "your_wifi_password"

# API Encryption (generate with: esphome config any-device.yaml and copy the suggested key)
api_encryption_key: "your_32_character_base64_key_here"

# OTA Password
ota_password: "your_ota_password"

# MQTT (if used)
mqtt_password: "your_mqtt_password"